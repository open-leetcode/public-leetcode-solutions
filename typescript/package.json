{
  "name": "public-leetcode-solutions",
  "version": "0.0.1",
  "description": "Public database of all LeetCode solutions",
  "scripts": {
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "mocha --exit --require ts-node/register test/**/*.ts --grep",
    "tests": "mocha --exit --require ts-node/register test/**/*.ts"
  },
  "bin": {
    "ts-boilerplate": "build/index.js"
  },
  "keywords": [
    "ts",
    "typescript"
  ],
  "author": "open-leetcode",
  "license": "MIT",
  "dependencies": {
    "commander": "8.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/commander": "2.12.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "17.0.8",
    "chai": "^4.3.4",
    "mocha": "9.1.3",
    "prettier": "^2.5.1",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
